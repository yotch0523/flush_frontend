{
  "$schema": "https://json.schemastore.org/staticwebapp.config.json",
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "b2c": {
          "registration": {
            "clientIdSettingName": "AAD_B2C_ClIENT_ID",
            "clientCredential": {
              "clientSecretSettingName": "AAD_B2C_ClIENT_SECRET"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://prodflush.b2clogin.com/prodflush.onmicrosoft.com/B2C_1_susi"
            }
          },
          "login": {
            "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
            "scopes": ["openid", "profile"]
          }
        }
      }
    }
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "networking": {
    "allowedIpRanges": ["126.23.223.98/32"]
  }
}
